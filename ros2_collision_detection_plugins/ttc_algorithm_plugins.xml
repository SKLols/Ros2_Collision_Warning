<library path="ttc_algorithm_plugins">
  <class type="ros2_collision_detection_plugins::CircleAlgorithm"
         base_class_type="ros2_collision_detection::TTCAlgorithm">
    <description>
      The Circle Algorithm with 1 circle per object.
    </description>
  </class>
  
  <class type="ros2_collision_detection_plugins::NCircleAlgorithm"
         base_class_type="ros2_collision_detection::TTCAlgorithm">
    <description>
      An extended version of the Circle Algorithm with n circles per object.
    </description>
  </class>
  

</library>
